{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "Task to implement simple CRUD API using in-memory database underneath",
  "author": "JsPowWow",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start:dev": "nodemon -w src src/index.ts",
    "start:prod": "npm run build && node dist/index.js",
    "start:multi": "nodemon -w src src/multi.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:verbose": "jest",
    "build": "tsc"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0"
  }
}
